{
  "name": "nuvana-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node dev.js",
    "dev:frontend": "next dev -p 3000",
    "dev:backend": "cd backend && npm run dev",
    "dev:old": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=backend/prisma/schema.prisma",
    "prepare": "npm run prisma:generate",
    "test:unit": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test vitest run --passWithNoTests tests/unit tests/component",
    "test:integration": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test vitest run tests/integration",
    "test:component": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test vitest run tests/component/",
    "test:component:layout": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test vitest run tests/component/DashboardLayout.test.tsx",
    "test:api:health": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api tests/api/backend-setup.api.spec.ts --grep \"Health Check\"",
    "test:api:infrastructure": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api tests/api/infrastructure.api.spec.ts",
    "test:e2e": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=e2e",
    "test:e2e:mobile": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=e2e tests/e2e/mobile-sidebar.spec.ts",
    "test:p0": "npm run test:api:infrastructure -- --grep \"\\[P0\\]\" && npm run test:e2e:mobile -- --grep \"\\[P0\\]\"",
    "test:smoke": "npm run test:api:infrastructure -- --grep \"\\[P0\\]\"",
    "test:e2e:ui": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=e2e --ui",
    "test:e2e:p0": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=e2e --grep \"\\[P0\\]\"",
    "test:e2e:health": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=e2e tests/e2e/health-check.spec.ts --grep \"\\[P0\\]\"",
    "test:e2e:p1": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=e2e --grep \"\\[P0\\]|\\[P1\\]\"",
    "test:api": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api",
    "test:api:ci": "cross-env CI=true DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api",
    "test:api:p0": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api --grep \"\\[P0\\]\"",
    "test:api:p1": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api --grep \"\\[P0\\]|\\[P1\\]\"",
    "test:api:p1:ci": "cross-env CI=true DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api --grep \"\\[P0\\]|\\[P1\\]\"",
    "test:api:p2": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api --grep \"\\[P0\\]|\\[P1\\]|\\[P2\\]\"",
    "test:api:ui": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api --ui",
    "test:api:error": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api tests/api/error-handling.api.spec.ts",
    "test:api:edge": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nuvana_test playwright test --project=api tests/api/edge-cases.api.spec.ts",
    "test:ci-local": "powershell -ExecutionPolicy Bypass -File scripts/test-ci-local.ps1",
    "test:ci-local:file": "powershell -ExecutionPolicy Bypass -File scripts/test-ci-local.ps1"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.11",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.555.0",
    "next": "^15.5.9",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "recharts": "^3.6.0",
    "redis": "^4.7.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/amqplib": "^0.10.8",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.1.4",
    "amqplib": "^0.10.9",
    "autoprefixer": "^10.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "^15.5.9",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^27.2.0",
    "postcss": "^8",
    "prisma": "^6.19.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0",
    "vite": "^7.2.4",
    "vitest": "^3.1.4",
    "wait-on": "^9.0.3"
  }
}
