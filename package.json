{
  "name": "nuvana-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node dev.js",
    "dev:frontend": "next dev -p 3000",
    "dev:backend": "cd backend && npm run dev",
    "dev:old": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=backend/prisma/schema.prisma",
    "prepare": "npm run prisma:generate",
    "test:unit": "vitest run --passWithNoTests tests/unit tests/component",
    "test:component": "vitest run tests/component/",
    "test:component:layout": "vitest run tests/component/DashboardLayout.test.tsx",
    "test:api:health": "playwright test --project=api tests/api/backend-setup.api.spec.ts --grep \"Health Check\"",
    "test:api:infrastructure": "playwright test --project=api tests/api/infrastructure.api.spec.ts",
    "test:e2e": "playwright test --project=e2e",
    "test:e2e:mobile": "playwright test --project=e2e tests/e2e/mobile-sidebar.spec.ts",
    "test:p0": "npm run test:api:infrastructure -- --grep \"\\[P0\\]\" && npm run test:e2e:mobile -- --grep \"\\[P0\\]\"",
    "test:smoke": "npm run test:api:infrastructure -- --grep \"\\[P0\\]\"",
    "test:e2e:ui": "playwright test --project=e2e --ui",
    "test:e2e:p0": "playwright test --project=e2e --grep \"[P0]\"",
    "test:e2e:health": "playwright test --project=e2e tests/e2e/health-check.spec.ts --grep \"[P0]\"",
    "test:e2e:p1": "playwright test --project=e2e --grep \"[P0]|[P1]\"",
    "test:api": "playwright test --project=api",
    "test:api:ci": "cross-env CI=true playwright test --project=api",
    "test:api:p0": "playwright test --project=api --grep \"[P0]\"",
    "test:api:p1": "playwright test --project=api --grep \"[P0]|[P1]\"",
    "test:api:p1:ci": "cross-env CI=true playwright test --project=api --grep \"[P0]|[P1]\"",
    "test:api:p2": "playwright test --project=api --grep \"[P0]|[P1]|[P2]\"",
    "test:api:ui": "playwright test --project=api --ui",
    "test:api:error": "playwright test --project=api tests/api/error-handling.api.spec.ts",
    "test:api:edge": "playwright test --project=api tests/api/edge-cases.api.spec.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.10",
    "@types/jsonwebtoken": "^9.0.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.468.0",
    "next": "^14.2.33",
    "next-themes": "^0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.66.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.0.1",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "jsdom": "^27.2.0",
    "postcss": "^8",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "vite": "^7.2.2",
    "vitest": "^3.1.4",
    "wait-on": "^9.0.3"
  }
}
